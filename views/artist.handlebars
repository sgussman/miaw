<html>
<head>
	<title>Artist</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, height=device-height">
	<link href="../css/bootstrap.css" rel="stylesheet"></link>
	<link href="../css/bootstrap-theme.css" rel="stylesheet"></link>
	<link href="../css/style.css" rel="stylesheet"></link>
</head>

<body>
	<div class="info_panel" id="artist_info_panel">
		{{#each artist}}
			<img src="{{artist_pi[@index]}}" alt="Mumbatu" class="profile_pic">
			<div class="artist_link_div">
				<p class="title" id="artist_name_big">{{this}}</p>
				<a href="{{website[@index]}}"><span class="invisible_mask"></span></a>
			</div>
		{{/each}}
	</div>
	<div class="textDiv">
		<p class="subtitle" id="artist_title">About the Artist</p>
		{{#each bio}}
			<p> {{this}} </p>
		{{/each}}
		{{>twitter}}
	</div>


{{>navBar}}

<script src="../js/jquery-1.11.0.js"></script>
<script src="../js/bootstrap.js"></script>

</body>
</html>